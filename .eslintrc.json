{
  "env": {
    "browser": true,
    "es2021": true
  },
  "extends": [
    "airbnb-base"
  ],
  "parserOptions": {
    "ecmaVersion": 12,
    "sourceType": "module"
  },
  "ignorePatterns": [
    "webpack.config.js",
    "postcss.config.js"
  ],
  "rules": {
    "import/no-extraneous-dependencies": 0
  }
}
